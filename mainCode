import java.util.Arrays;

public class multiplicationTable {

    public static void main(String[] args){

        int [][] myArray;

        myArray = new int[10][10];

        for(int i=0; i<10; i++){
            for(int j=0; j<10; j++){
                myArrayk[i][j] = (i+1)*(j+1);
